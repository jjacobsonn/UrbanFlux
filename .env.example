# Example environment variables for UrbanFlux
# Copy this file to .env and fill in your actual values

# PostgreSQL Database Configuration
PGHOST=localhost
PGPORT=5432
PGUSER=urbanflux_user
PGPASSWORD=change_me_in_production
PGDATABASE=urbanflux

# ETL Configuration
ETL_INPUT_PATH=./testdata/sample.csv
ETL_CHUNK_SIZE=100000
ETL_MODE=full

# Logging
RUST_LOG=urbanflux=info,sqlx=warn
